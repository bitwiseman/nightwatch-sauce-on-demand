{
  "src_folders": [
    "tests"
  ],
  "output_folder": "reports",
  "custom_commands_path": "custom_commands",
  "custom_assertions_path": "",
  "page_objects_path": "page_objects",
  "globals_path": "globalsModule.js",
  "selenium": {},
  "test_settings": {
    "default": {
      "launch_url": "http://admanagerplus.yahoo.com",
      "selenium_port": 80,
      "selenium_host": "ondemand.saucelabs.com",
      "username": "${SAUCE_USERNAME}",
      "access_key": "${SAUCE_ACCESS_KEY}",
      "silent": true,
      "screenshots": {
        "enabled": true,
        "on_failure": true,
        "path": "screenshots/"
      },
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true
      }
    },
    "chrome_os_x": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "platform": "OS X 10.11",
        "version": "53.0",
        "javascriptEnabled": true,
        "acceptSslCerts": true
      },
      "screenshots": {
        "enabled": false,
        "on_failure": true,
        "on_error": false,
        "path": ""
      }
    },
    "firefox_os_x": {
      "desiredCapabilities": {
        "browserName": "firefox",
        "platform": "OS X 10.11",
        "version": "48.0"
      }
    }
  }
}
